        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">arom</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">poly</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">delo</enumeration>

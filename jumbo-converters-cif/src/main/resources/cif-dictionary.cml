        </definition>
        <superclass xmlns="">chemical</superclass>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">CD</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">EC</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">other</enumeration>
    </entry>
    <entry term="chemical_melting_point" id="chemical_melting_point" dataType="xsd:double" units="si:k" unitType="unitType:temperature">

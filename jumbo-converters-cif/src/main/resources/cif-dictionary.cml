    <entry term="publ_body_title" id="publ_body_title" dataType="xsd:string">
        <description>
            <xhtml:p>Corresponds to the _publ_body_title term in the IUCr Core CIF dictionary.</xhtml:p>
        </description>
        <definition>
            <xhtml:p> Title of the associated section of text. </xhtml:p>
        </definition>
        <superclass xmlns="">publ_body</superclass>
    </entry>
    <entry term="publ_manuscript_incl_extra_defn" id="publ_manuscript_incl_extra_defn" dataType="xsd:string">
        <description>
            <xhtml:p>Corresponds to the _publ_manuscript_incl_extra_defn term in the IUCr Core CIF dictionary.</xhtml:p>
        </description>
        <definition>
            <xhtml:p> Flags whether the corresponding data item marked for inclusion in a journal request list is a standard CIF definition or not. </xhtml:p>
        </definition>

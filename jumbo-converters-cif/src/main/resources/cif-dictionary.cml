        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">ascii</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">cif</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">latex</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">rtf</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">sgml</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">tex</enumeration>
        <enumeration xmlns="http://www.xml-cml.org/schema/cmlx">troff</enumeration>
    </entry>
    <entry term="publ_body_label" id="publ_body_label" dataType="xsd:string">
        <description>
            <xhtml:p>Corresponds to the _publ_body_label term in the IUCr Core CIF dictionary.</xhtml:p>
        </description>

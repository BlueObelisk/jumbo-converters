<template repeatCount="*"  id="nucdipole" name="Nuclear Dipole moment" 
  pattern="\s*Nuclear Dipole moment.*$\s+\-+.*" multiple="$" endPattern=".*$\s*\-+\s*">
  <comment>
            Nuclear Dipole moment (a.u.) 
            ----------------------------
        X                 Y               Z
 ---------------- ---------------- ----------------
     0.0000000000     0.0000000000     0.0000000000
  </comment>
  <record repeatCount="4">{X}</record>
  <record formatType="REGEX" id="nucdipole">{F,n:x}{F,n:y}{F,n:z}</record>
</template>

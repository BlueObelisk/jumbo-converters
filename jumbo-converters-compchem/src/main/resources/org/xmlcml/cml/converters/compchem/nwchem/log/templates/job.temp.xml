<template repeatCount="*"  name="Job information" id="job" 
  pattern=" *Job information *$\s+\-+.*" multiple="$" endPattern=".*$\s*\-+\s*"
  >
  <comment>
               Job information
               ---------------
    
        hostname      = arcen
        program       = /home/d3y133/nwchem-releases/nwchem-6.0-gfortran/bin/LINUX64/nwchem
        date          = Fri Aug  6 13:47:30 2010
    
        compiled      = Fri_Aug_06_13:42:42_2010
        source        = /home/d3y133/nwchem-releases/nwchem-6.0-gfortran
        nwchem branch = 6.0
        input         = dft_feco5.nw
        prefix        = dft_feco5.
        data base     = ./dft_feco5.db
        status        = startup
        nproc         =        4
        time left     =     -1s

    </comment>
  <record repeatCount="3">{X}</record>
  <record id="list1" repeatCount="*">\s*{X,n:name}\s*\=\s*{X,n:value}\s*</record>
  <record>{X}</record>
  <record id="list2" repeatCount="*">\s*{X,n:name}\s*\=\s*{X,n:value}\s*</record>
  <transform process="nameValue" parent="cml:list[@templateRef='list2']/cml:list" 
      name="cml:scalar[@dictRef='n:name']" value="cml:scalar[@dictRef='n:value']"/>
  <transform process="nameValue" parent="cml:list[@templateRef='list1']/cml:list" 
      name="cml:scalar[@dictRef='n:name']" value="cml:scalar[@dictRef='n:value']"/>
  <transform process="pullupSingleton" name="./*[@templateRef='list1' or @templateRef='list2']/cml:list"/>
      <!-- 
  <transform process="rename"    from="nwchem branch" value="branch"/>
  <transform process="rename"    from="data base" value="database"/>
  <transform process="rename"    from="time left" value="timeLeft"/>
  <transform process="dataType" name="nproc" value="xsd:integer"/>
  <transform process="dataType" name="compiled" value="xsd:date"/>
  <transform process="dataType" name="date" value="xsd:date"/>
  -->
</template>

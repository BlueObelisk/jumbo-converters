<template id="gau:mulliken" name="Mulliken atomic charges" 
    pattern=".*Mulliken atomic charges.*" endPattern=".*Sum of Mulliken charges=.*" endOffset="1">
  <comment class="example">
 Mulliken atomic charges:
              1
     1  Si   1.103178
     2  O   -0.481658
     ...
    15  H    0.254933
 Sum of Mulliken charges=   0.00000
 </comment>
  <record formatType="REGEX" id="txt">{X,zz:z}</record>
  <record formatType="REGEX" id="serial">.{10}{I5,gau:col}</record>
  <record formatType="REGEX" id="charges"  makeArray="true" 
      repeatCount="*">{I,gau:ser} {A,gau:elem} {F,gau:charge}</record>
  <record id="rlx" formatType="REGEX">.*Sum of Mulliken charges= *{F,gau:sum}.*</record>
</template>
 
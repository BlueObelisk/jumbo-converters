    </module>
  </comment>
  
  <comment class="example.output" id="l202.distmat.2">
    <module cmlx:templateRef="l202.distmat" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx">

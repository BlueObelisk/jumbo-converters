<template
  id="composite.summary.G4MP2"
  name="Composite Method G4MP2 - Summary"
  repeat="*"
  newline="$" 
  pattern="\s*Temperature=.+$.+$.+$.+$.+$.+$\s+1\\1\\GINC.+\\Mixed\\G4MP2\\.+"
  offset="0"
  endPattern=".+\\.+"
  endOffset="-1"
  xmlns:xi="http://www.w3.org/2001/XInclude">

  <comment class="example.input" id="composite.summary.G4MP2">
 Temperature=              298.150000 Pressure=                      1.000000
 E(ZPE)=                     0.050464 E(Thermal)=                    0.053793
 E(CCSD(T))=              -115.374477 E(Empiric)=                   -0.066304
 DE(MP2)=                   -0.167765 DE(HF)=                       -0.012963
 G4MP2(0 K)=              -115.571046 G4MP2 Energy=               -115.567717
 G4MP2 Enthalpy=          -115.566773 G4MP2 Free Energy=          -115.593790
  </comment>

  <record id="G4MP2.Temp.Press" repeat="1">\s*Temperature=\s*{F,cc:temperature}\s+Pressure=\s*{F,cc:pressure}\s*</record>
  <record id="G4MP2.contrib1"   repeat="1">\s*E\(ZPE\)=\s*{F,g:E_ZPE}\s+E\(Thermal\)=\s*{F,g:E_Thermal}\s*</record>
  <record id="G4MP2.contrib2"   repeat="1">\s*E\(CCSD\(T\)\)=\s*{F,g:E_CCSD_T}\s+E\(Empiric\)=\s*{F,g:E_Empiric}\s*</record>
  <record id="G4MP2.contrib3"   repeat="1">\s*DE\(MP2\)=\s*{F,g:DE_MP2}\s+DE\(HF\)=\s*{F,g:DE_HF}\s*</record>
  <record id="G4MP2a"           repeat="1">\s*G4MP2\(0 K\)=\s*{F,g:Energy_G4MP2_0K}\s+G4MP2 Energy=\s*{F,g:Energy_G4MP2}\s*</record>
  <record id="G4MP2b"           repeat="1">\s*G4MP2 Enthalpy=\s*{F,g:Enthalpy_G4MP2}\s+G4MP2 Free Energy=\s*{F,g:GibbsEnergy_G4MP2}\s*</record>

  <!-- Pull up scalars -->
  <transform process="pullup" xpath=".//cml:list/cml:list/cml:scalar[@dictRef]"  />
  <transform process="pullup" xpath=".//cml:list/cml:scalar[@dictRef]" />


  <!-- Delete empty lists -->
  <transform process="delete" xpath=".//cml:list[count(*)=0]"/>
  <transform process="delete" xpath=".//cml:list[count(*)=0]"/>

  <!-- Improve some dictrefs -->
  <transform process="addDictRef" xpath="./cml:scalar[@dictRef='g:E_ZPE']"             value="cc:Energy_ZPVE"/>
  <transform process="addDictRef" xpath="./cml:scalar[@dictRef='g:E_Thermal']"         value="cc:thermalCorrectionToEnergy"/>
  <transform process="addDictRef" xpath="./cml:scalar[@dictRef='g:E_CCSD_T']"          value="g:E_CCSD_T"/>
  <transform process="addDictRef" xpath="./cml:scalar[@dictRef='g:Energy_G4MP2_0K']"   value="cc:Energy_0K"/>
  <transform process="addDictRef" xpath="./cml:scalar[@dictRef='g:Energy_G4MP2']"      value="cc:Energy_T"/>
  <transform process="addDictRef" xpath="./cml:scalar[@dictRef='g:Enthalpy_G4MP2']"    value="cc:Enthalpy_T"/>
  <transform process="addDictRef" xpath="./cml:scalar[@dictRef='g:GibbsEnergy_G4MP2']" value="cc:GibbsEnergy_T"/>
  
  <!-- Add units -->
  <transform process="addUnits" xpath="./cml:scalar[@dictRef]" value="nonSi:hartree"/>
  <transform process="addUnits" xpath="./cml:scalar[@dictRef='cc:temperature']" value="si:kelvin"/>
  <transform process="addUnits" xpath="./cml:scalar[@dictRef='cc:pressure']"    value="nonSi:atm"/>

  <!-- separate parameters and properties -->
  <transform process="addChild" elementName="cml:parameterList" xpath="." position="0" />
  <transform process="addChild" elementName="cml:propertyList" xpath="." position="1" />

  <transform process="move" xpath="./cml:scalar[@dictRef='method' or @dictRef='cc:temperature' or @dictRef='cc:pressure']" to="./cml:parameterList" />
  <transform process="move" xpath="./cml:scalar[contains(@dictRef,'cc:') or contains(@dictRef,'g:')]"                      to="./cml:propertyList" />

  <transform process="createWrapperParameter" xpath="./cml:parameterList/cml:scalar" />
  <transform process="createWrapperProperty"  xpath="./cml:propertyList/cml:scalar" />

  <comment class="example.output" id="composite.summary.G4MP2">
    <module xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx" cmlx:templateRef="composite.summary.G4MP2">
      <parameterList>
        <parameter dictRef="cc:temperature">
          <scalar dataType="xsd:double" units="si:kelvin">298.15</scalar>
        </parameter>
        <parameter dictRef="cc:pressure">
          <scalar dataType="xsd:double" units="nonSi:atm">1.0</scalar>
        </parameter>
      </parameterList>
      <propertyList>
        <property dictRef="cc:Energy_ZPVE">
          <scalar dataType="xsd:double" units="nonSi:hartree">0.050464</scalar>
        </property>
        <property dictRef="cc:thermalCorrectionToEnergy">
          <scalar dataType="xsd:double" units="nonSi:hartree">0.053793</scalar>
        </property>
        <property dictRef="g:E_CCSD_T">
          <scalar dataType="xsd:double" units="nonSi:hartree">-115.374477</scalar>
        </property>
        <property dictRef="g:E_Empiric">
          <scalar dataType="xsd:double" units="nonSi:hartree">-0.066304</scalar>
        </property>
        <property dictRef="g:DE_MP2">
          <scalar dataType="xsd:double" units="nonSi:hartree">-0.167765</scalar>
        </property>
        <property dictRef="g:DE_HF">
          <scalar dataType="xsd:double" units="nonSi:hartree">-0.012963</scalar>
        </property>
        <property dictRef="cc:Energy_0K">
          <scalar dataType="xsd:double" units="nonSi:hartree">-115.571046</scalar>
        </property>
        <property dictRef="cc:Energy_T">
          <scalar dataType="xsd:double" units="nonSi:hartree">-115.567717</scalar>
        </property>
        <property dictRef="cc:Enthalpy_T">
          <scalar dataType="xsd:double" units="nonSi:hartree">-115.566773</scalar>
        </property>
        <property dictRef="cc:GibbsEnergy_T">
          <scalar dataType="xsd:double" units="nonSi:hartree">-115.59379</scalar>
        </property>
      </propertyList>
    </module>
  </comment>

</template>
<template id="l4601.occupied" 
		  repeat="*"
		  pattern="\s*Occupied\s*\(.*"
  		  endPattern="\s*Virtual\s*\(.*" endOffset="0">
  
  <record id="occ" repeat="*" makeArray="true">\s*\w*\s*{1_20A,g:occorbs}</record>

  <transform process="addDictRef" xpath=".//cml:array[@dictRef='g:occorbs']" value="cc:occOrbitalSymmetry" />
  
</template>

  
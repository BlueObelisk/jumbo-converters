<template id="l716.thermochemistry.vibtemp" repeat="*" pattern="\s*V[Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Aa][lL] [Tt][Ee][Mm][Pp][Ee][Rr][Aa][Tt][Uu][Rr][Ee][Ss].*" endPattern="\s*">

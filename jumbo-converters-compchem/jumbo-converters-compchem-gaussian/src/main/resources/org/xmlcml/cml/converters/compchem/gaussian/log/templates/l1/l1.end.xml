<template id="l1.end" name="link1" repeat="*" newline="$"
    pattern="\s\*+\s*$\s*Gaussian\s*\d+:.*" 
    endPattern=".*99.*\;\s*$\s[^/]+" endOffset="1"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    >
    
  <comment class="example.input" id="l1.end">

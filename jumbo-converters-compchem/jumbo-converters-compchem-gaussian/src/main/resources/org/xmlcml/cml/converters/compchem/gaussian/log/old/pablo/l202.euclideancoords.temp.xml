<template id="t_euclideancoords" pattern="\s+Standard\sorientation.*" 
 endPattern="\sRotational\sconstants.*">
	<record repeatCount="5">{X}</record>
	<record repeatCount="*" id="r_euclideancoords" makeArray="true"
    >{I,gaussian:atomindex} {I,cml:elementType} {I,gaussian:atomtype} {F,cml:x3} {F,cml:y3} {F,cml:z3}</record>
    <transform process="createMolecule" xpath="cml:list/cml:array"/>    
</template>
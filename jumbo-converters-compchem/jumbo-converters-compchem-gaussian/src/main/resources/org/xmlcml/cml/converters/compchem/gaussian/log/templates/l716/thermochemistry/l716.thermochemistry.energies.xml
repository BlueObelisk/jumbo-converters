<template id="l716.thermochemistry.energies" name="Thermochemistry Energies" repeat="*"
    pattern="\s*Zero-point correction.*" 
    endPattern="\s*" endOffset="1"
    >
    
  <comment class="example.input" id="l716.thermochemistry.energies">
 Zero-point correction=                           0.045230 (Hartree/Particle)
 Thermal correction to Energy=                    0.048094
 Thermal correction to Enthalpy=                  0.049039
 Thermal correction to Gibbs Free Energy=         0.027907
 Sum of electronic and zero-point Energies=            -40.473159
 Sum of electronic and thermal Energies=               -40.470295
 Sum of electronic and thermal Enthalpies=             -40.469351
 Sum of electronic and thermal Free Energies=          -40.490482
 
  </comment>

  <record id="thermo.ZeroPointEnergy" repeat="*">\s*Zero-point correction=\s*{F,cc:Energy_ZPVE,u:hartree}.+</record>
  <record id="thermo.thermCorrEnergy" repeat="*">\s*Thermal correction to Energy=\s*{F,cc:thermalCorrectionToEnergy,u:hartree}.+</record>
  <record id="thermo.thermCorrEnthal" repeat="*">\s*Thermal correction to Enthalpy=\s*{F,cc:thermalCorrectionToEnthalpy,u:hartree}.+</record>
  <record id="thermo.thermCorrFreeEn" repeat="*">\s*Thermal correction to Gibbs Free Energy=\s*{F,cc:thermalCorrectionToGibbsEnergy,u:hartree}.+</record>
  <record id="thermo.sumElZeroPointE" repeat="*">\s*Sum of electronic and zero-point Energies=\s*{F,cc:Energy_0K,u:hartree}.+</record>
  <record id="thermo.sumElTherEnergy" repeat="*">\s*Sum of electronic and thermal Energies=\s*{F,cc:Energy_T,u:hartree}.+</record>
  <record id="thermo.sumElTherEnthal" repeat="*">\s*Sum of electronic and thermal Enthalpies=\s*{F,cc:Enthalpy_T,u:hartree}.+</record>
  <record id="thermo.sumElTherFreeEn" repeat="*">\s*Sum of electronic and thermal Free Energies=\s*{F,cc:GibbsEnergy_T,u:hartree}.+</record>

  <transform process="pullup" xpath=".//cml:scalar" /> 

  <!-- add units -->
  <transform process="addUnits" xpath=".//cml:scalar[@dictRef='cc:Energy_ZPVE']" 
      value="nonSi:hartree" dataType="xsd:double"/>
  <transform process="addUnits" xpath=".//cml:scalar[@dictRef='cc:thermalCorrectionToEnergy']" 
      value="nonSi:hartree"/>
  <transform process="addUnits" xpath=".//cml:scalar[@dictRef='cc:thermalCorrectionToEnthalpy']" 
      value="nonSi:hartree"/>
  <transform process="addUnits" xpath=".//cml:scalar[@dictRef='cc:thermalCorrectionToGibbsEnergy']" 
      value="nonSi:hartree"/>
  <transform process="addUnits" xpath=".//cml:scalar[@dictRef='cc:Energy_0K']" 
      value="nonSi:hartree"/>
  <transform process="addUnits" xpath=".//cml:scalar[@dictRef='cc:Energy_T']" 
      value="nonSi:hartree"/>
  <transform process="addUnits" xpath=".//cml:scalar[@dictRef='cc:Enthalpy_T']" 
      value="nonSi:hartree"/>
  <transform process="addUnits" xpath=".//cml:scalar[@dictRef='cc:GibbsEnergy_T']" 
      value="nonSi:hartree"/>

 <!--  delete empty lists -->
 <transform process="delete" xpath=".//cml:list[count(*)=0]"/>
 <transform process="delete" xpath=".//cml:list[count(*)=0]"/>

  <comment class="example.output" id="l716.thermochemistry.energies">
<module xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx" 
    cmlx:templateRef="l716.thermochemistry.energies" >
    <scalar dataType="xsd:double" units="nonSi:hartree" dictRef="cc:Energy_ZPVE"                    >0.04523</scalar>
    <scalar dataType="xsd:double" units="nonSi:hartree" dictRef="cc:thermalCorrectionToEnergy"      >0.048094</scalar>
    <scalar dataType="xsd:double" units="nonSi:hartree" dictRef="cc:thermalCorrectionToEnthalpy"    >0.049039</scalar>
    <scalar dataType="xsd:double" units="nonSi:hartree" dictRef="cc:thermalCorrectionToGibbsEnergy" >0.027907</scalar>
    <scalar dataType="xsd:double" units="nonSi:hartree" dictRef="cc:Energy_0K"                      >-40.473159</scalar>
    <scalar dataType="xsd:double" units="nonSi:hartree" dictRef="cc:Energy_T"                       >-40.470295</scalar>
    <scalar dataType="xsd:double" units="nonSi:hartree" dictRef="cc:Enthalpy_T"                     >-40.469351</scalar>
    <scalar dataType="xsd:double" units="nonSi:hartree" dictRef="cc:GibbsEnergy_T"                  >-40.490482</scalar>
  </module>
  </comment>
</template>
 
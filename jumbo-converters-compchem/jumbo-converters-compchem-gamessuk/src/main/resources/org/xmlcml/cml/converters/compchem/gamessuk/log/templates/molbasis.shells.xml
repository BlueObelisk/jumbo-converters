<template id="molbasis.shells" pattern="\stotal number of shells.*\s*" endPattern="\s*">
  <comment>
 total number of shells                  10
 total number of basis functions         22
 number of electrons                     16
 charge of molecule                       0
 state multiplicity                       1
 number of occupied orbitals (alpha)      8
 number of occupied orbitals (beta )      8
 total number of atoms                    4

  </comment>
  <record>\stotal number of shells\s+{I,guk:a}\s*</record>
  <record>\stotal number of basis functions\s+{I,guk:a}\s*</record>
  <record>\snumber of electrons\s+{I,guk:a}\s*</record>
  <record>\scharge of molecule\s+{I,guk:a}\s*</record>
  <record>\sstate multiplicity\s+{I,guk:a}\s*</record>
  <record>\snumber of occupied orbitals \(alpha\)\s+{I,guk:a}\s*</record>
  <record>\snumber of occupied orbitals \(beta \)\s+{I,guk:a}\s*</record>
  <record>\stotal number of atoms\s+{I,guk:a}\s*</record>
  <record repeat="*">{X,x:sss}</record>
  
  <transform process="pullupSingleton" xpath="./cml:list"/>
  
</template>

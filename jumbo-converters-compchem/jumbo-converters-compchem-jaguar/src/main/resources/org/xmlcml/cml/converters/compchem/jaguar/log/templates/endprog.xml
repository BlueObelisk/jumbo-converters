  end of program ch
  </comment>
  <record id="endprog">\s*end of program.*</record>
  
  <comment class="example.output" id="endprog">
    <module cmlx:templateRef="endprog" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx"/>
  </comment>
</template>

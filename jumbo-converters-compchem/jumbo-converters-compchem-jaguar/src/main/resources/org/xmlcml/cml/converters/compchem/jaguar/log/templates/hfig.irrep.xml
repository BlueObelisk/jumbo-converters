<template name="irrep" id="irrep" newline="$" 
    pattern="\s*Irreducible.*$\s*represent.*" endPattern="\s*">
    <record repeat="2"/>
    <record name="irrep" id="irrep">{X,j:irrep}{I,j:totorb}{I,j:shell1}</record>
    <record/>
    <record id="occ">\s*Orbital occupation/shell{F,cc:occ}\s*</record>
</template>

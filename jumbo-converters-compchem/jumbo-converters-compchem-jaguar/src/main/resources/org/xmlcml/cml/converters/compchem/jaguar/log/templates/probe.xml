<template repeatCount="*"  id="probe" name="probe"
  multiple="$"
  pattern="\s*start of program probe\s*"
  endPattern="\s*end of program probe\s*" endOffset="1"
  >
  <comment class="example.input" id="probe">
  start of program probe
  end of program probe
  </comment>
  <record repeat="2"/>
  
  <comment class="example.output" id="probe">
    <module cmlx:templateRef="probe" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx"/>
  </comment>
  
</template>

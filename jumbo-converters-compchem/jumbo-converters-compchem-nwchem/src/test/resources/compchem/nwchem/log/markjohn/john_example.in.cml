<module id ="john_fukui" dictRef="compchem:inputList" convention="convention:compchem" title="John's example for Fukui" 
  xmlns="http://www.xml-cml.org>
  
  <molecule id="molecule" formalCharge="0" spinMultiplicity="1">
      <atomArray>
        ...
      </atomArray>
  </molecule>
 
  <module id="dft" dictRef="compchem:input" convention="convention:compchem" title="John's example for Fukui" 
  xmlns="http://www.xml-cml.org>
   <parameterList>
     <parameter dictRef="cc:basis">
       <atomType>H</atomType>
       <scalar dataType="xsd:string">6-31g**</scalar>
     </parameter>
     <parameter dictRef="cc:basis">
       <atomType>C</atomType>
       <scalar dataType="xsd:string">6-31g**</scalar>
     </parameter>
     <parameter dictRef="cc:basis">
       <atomType>N</atomType>
       <scalar dataType="xsd:string">6-31g**</scalar>
     </parameter>
     <parameter dictRef="cc:basis">
       <atomType>O</atomType>
       <scalar dataType="xsd:string">6-31g**</scalar>
     </parameter>
     <parameter dictRef="cc:dft">
       <scalar dataType="xsd:string" dictRef="cc:dft.qualifier">xc</scalar>
       <scalar dataType="xsd:string" >b3lyp</scalar>
     </parameter>
     <parameter dictRef="cc:calculation">
       <scalar dataType="xsd:string">fukui</scalar>
     </parameter>
     <parameter dictRef="cc:output">
       <scalar dataType="xsd:string">verbose</scalar>
     </parameter>
     <parameter dictRef="cc:calculation">
       <scalar dataType="xsd:string">dft</scalar>
     </parameter>
   </parameterList>
  </module>  

<!--
 start john_fukui
 title "John's example for Fukui"
 
 geometry units an noautoz nocenter noautosym
   C    -2.7874000000  1.0093000000  0.0350000000
   C    -3.3765000000 -0.2605000000  0.0490000000
   H     1.6375000000  2.7694000000 -1.3506000000
   H     0.2796000000  3.0270000000 -0.2295000000
 end
 basis
   H library 6-31g**
   O library 6-31g**
   C library 6-31g**
   N library 6-31g**
 end
 dft
   xc b3lyp
 end
 set dft:condfukui 1
 print high
 task dft
-->
</module>  


<templateList>
  <template id="stretch" repeat="*" pattern=".*\sStretch.*" endPattern=".*" endOffset="1">
    <record id="stretch" repeat="*" 
     >\s*{I,n:serial}\s+Stretch\s*{I,n:i}\s*{I,n:j}\s*{F,n:len,u:angst}{F,n:grad,u:angst}</record>
  </template>
  <template id="stretch1" repeat="*" pattern=".*\*Stretch.*" endPattern=".*" endOffset="1">
    <record id="stretch1" repeat="*" 
     >\s*{I,n:serial}\s*\*Stretch\s*{A,n:name}\s{I,n:i}\s*{I,n:j}\s{F,n:len,u:angst}{F,n:grad,u:angst}</record>
  </template>
  <template id="bend" repeat="*" pattern=".*\sBend.*" endPattern=".*" endOffset="1">
    <record id="bend" repeat="*"
         >\s*{I,n:serial}\s+Bend\s*{I,n:i}\s*{I,n:j}\s*{I,n:k}\s*{F,n:ang,u:deg}{F,n:grad,u:deg}</record>
  </template>
  <template id="bend1" repeat="*" pattern=".*\*Bend.*" endPattern=".*" endOffset="1">
    <record id="bend1" repeat="*"
         >\s*{I,n:serial}\s*Bend\s*{A,n:name}\s{I,n:i}\s*{I,n:j}\s*{I,n:k}\s*{F,n:ang,u:deg}{F,n:grad,u:deg}</record>
  </template>
  <template id="torsion" repeat="*" pattern=".*\sTorsion.*" endPattern=".*" endOffset="1">
    <record id="torsion" repeat="*"
      >\s*{I,n:serial}\s+Torsion\s*{I,n:i}\s*{I,n:j}\s*{I,n:k}\s*{I,n:l}\s*{F,n:tor,u:deg}{F,n:grad,u:deg}</record>
  </template>
  <template id="torsion1" repeat="*" pattern=".*\*Torsion.*" endPattern=".*" endOffset="1">
    <record id="torsion1" repeat="*"
      >\s*{I,n:serial}\s*Torsion\s*{I,n:i}\s*{I,n:j}\s*{I,n:k}\s*{I,n:l}\s*{F,n:tor,u:deg}{F,n:grad,u:deg}</record>
  </template>
</templateList>
  

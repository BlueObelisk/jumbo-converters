<template repeat="*"  name="Parallel foo file" id="parallel" 
  pattern="\s*Parallel .* file.*" newline="$" endPattern="\s*">

  <comment class="example.input" id="parallel">
Parallel integral file used 8 records with 0 large values
Parallel grid_pts file used 8 records with 0 large values

  </comment>
  <record id="parallel">\s*Parallel {X,cc:filetype} file used\s*{I,n:nrec}\s*records with\s*{I,n:large}\s*large values\s*</record>

  <comment class="example.output" id="parallel">
<module cmlx:templateRef="parallel" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx">
  <list cmlx:templateRef="parallel">
    <list>
      <scalar dataType="xsd:string" dictRef="cc:filetype">integral</scalar>
      <scalar dataType="xsd:integer" dictRef="n:nrec">8</scalar>
      <scalar dataType="xsd:integer" dictRef="n:large">0</scalar>
    </list>
  </list>Parallel grid_pts file used 8 records with 0 large values
</module>
  </comment>

</template>

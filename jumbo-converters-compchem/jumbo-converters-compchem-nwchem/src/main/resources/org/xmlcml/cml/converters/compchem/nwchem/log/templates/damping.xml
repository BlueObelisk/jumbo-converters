<template repeat="*"  id="damping" name="damping" 
  pattern="\s*DAMPING=.*$.*" 
  newline="$" endPattern=".*">

  <comment class="example.input" id="damping">
           DAMPING=T LEVELSHIFTING=F DIISING=T
          itol2e= 6 iAOacc=12 iXCacc=20
  </comment>
  <record repeat="2"/>
  <comment class="example.output" id="damping">
    <module cmlx:templateRef="damping" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx"/>
  </comment>

</template>

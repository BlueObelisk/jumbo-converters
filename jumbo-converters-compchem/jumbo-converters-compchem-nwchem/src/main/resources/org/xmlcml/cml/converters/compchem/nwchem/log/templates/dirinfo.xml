<template repeat="*"  id="dirinfo" name="Directory information" pattern="\s*Directory information\s*$\s+\-+.*" newline="$" endPattern=".*$\s*\-+\s*">
  <comment>
    Directory information
    ---------------------

    0 permanent = .
    0 scratch = .

</comment>
  <record repeat="3">{X}</record>
  <record repeat="*">\s*{I,n:unknown}{A,n:disk}\s*\=\s*{X,n:directory}</record>
</template>

  <transform process="move" xpath="./*[@role='group'][1]" toXpath="//*[@id='initial']"/>
  <transform process="addRole" xpath="./*[@role='group'][1]" value="initialization"/>
  <transform process="addRole" xpath="./*[@role='group'][count(//*[@role='group'])]" value="finalization"/>
  <transform process="addDictRef" xpath="./*[@role='initialization'][1]" value="compchem:initialization"/>

<template repeat="*"  id="nucdipole" name="Nuclear Dipole moment" 
  pattern="\s*Nuclear Dipole moment.*$\s+\-+\s*" newline="$" endPattern="\s*">
  <comment>
            Nuclear Dipole moment (a.u.) 
            ----------------------------
        X                 Y               Z
 ---------------- ---------------- ----------------
     0.0000000000     0.0000000000     0.0000000000
     
  </comment>
  <record repeat="4">{X}</record>
  <record id="nucdipole">{F,x:x}{F,x:y}{F,x:z}</record>
  <transform process="pullupSingleton" xpath="./cml:list[count(*)=1]"/> 
  <transform process="createVector3" xpath="./*[@cmlx:templateRef='nucdipole']" 
      from="*[@dictRef='x:x' or @dictRef='x:y' or @dictRef='x:z']"
      dictRef="cc:nuclearDipole"/>
<!-- <transform process="pullupSingleton" xpath="./cml:list"/> -->
</template>

<template repeat="*"  id="nuclear_dipole_moment" name="Nuclear Dipole moment" 
  pattern="\s*Nuclear Dipole moment.*$\s+\-+\s*" newline="$" endPattern="\s*">

  <comment class="example.input" id="nuclear_dipole_moment">
            Nuclear Dipole moment (a.u.) 
            ----------------------------
        X                 Y               Z
 ---------------- ---------------- ----------------
     0.0000000000     0.0000000000     0.0000000000
     
  </comment>
  <record repeat="4"/>
  <record id="nucdipole">{3F,compchem:nuclear_dipole_moment}</record>
  <transform process="pullup" xpath=".//cml:array"/> 
  <transform process="delete" xpath=".//cml:list[@cmlx:templateRef='missingID']"/>
  <transform process="delete" xpath=".//cml:list[@cmlx:templateRef='nucdipole']"/>
  

  <comment class="example.output" id="nuclear_dipole_moment">
    <module cmlx:templateRef="nuclear_dipole_moment" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx">
      <array dataType="xsd:double" size="3" dictRef="compchem:nuclear_dipole_moment">0.0 0.0 0.0</array>
    </module>
  </comment>

</template>

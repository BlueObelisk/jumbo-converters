<template repeat="*"  id="energycalc" name="energycalc" 
  pattern="\s*\=+\s*[Ee]nergy [Cc]alculation\s*==?\s*" 
  newline="$" endPattern="\s*>>>\s*ITERATION\s*ENDED.*" endOffset="1">
  

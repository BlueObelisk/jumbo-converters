    <scalar dataType="xsd:double" dictRef="n:xmax">0.0</scalar>
    <scalar dataType="xsd:double" dictRef="compchem:wallTime" units="si:s">98.3</scalar>
  </list>
  <module cmlx:lineCount="1" cmlx:templateRef="ok">
    <scalar dataType="xsd:string" dictRef="n:unparsed">ok       ok</scalar>
  </module>
</module>
  </comment>
   

<template repeat="*"  id="nucdipole" name="Nuclear Dipole moment" 
  pattern="\s*Nuclear Dipole moment.*$\s+\-+.*" newline="$" endPattern=".*$\s*\-+\s*">
  <comment>
            Nuclear Dipole moment (a.u.) 
            ----------------------------
        X                 Y               Z
 ---------------- ---------------- ----------------
     0.0000000000     0.0000000000     0.0000000000
  </comment>
  <record repeat="4">{X}</record>
  <record id="nucdipole">{F,n:x}{F,n:y}{F,n:z}</record>
  <transform process="pullupSingleton" xpath="./cml:list[count(*)=1]"/> 
  <transform process="vector3" xpath="./*[@templateRef='nucdipole']" 
      from="*[@dictRef='n:x' or @dictRef='n:y' or @dictRef='n:z']"
      dictRef="compchem:nuclearDipole"/>
<!-- <transform process="pullupSingleton" xpath="./cml:list"/> -->
</template>

<template repeat="*"  id="caching" name="Caching" 
  pattern="\s*Caching 1\-el integrals.*"  endPattern=".*">
  
  <comment class="example.input" id="caching">
  Caching 1-el integrals 
</comment>

  <record id="caching" >\s*{A,n:caching}.*\s*</record>
  
  <comment class="example.output" id="caching">
    <module cmlx:templateRef="caching" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx">
      <list cmlx:templateRef="caching">
        <scalar dataType="xsd:string" dictRef="n:caching">Caching</scalar>
      </list>
    </module>
  </comment>

</template>

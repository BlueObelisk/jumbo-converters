<template id="qchem.timestamp"
  repeat="*"
  name="qchem.timestamp"
  pattern="\s*Q-chem begins on.+"      offset="0"
  endPattern=".+"   endOffset="0" >

  <comment class="example.input" id="qchem.timestamp">
 Q-chem begins on Wed May 27 15:12:18 2015  
  </comment>

  <record id="qchem.timestamp">\s*Q-chem begins on {X,cc:runDate}\s*</record>

  <transform process="createDate"
             xpath=".//cml:list[@cmlx:templateRef='qchem.timestamp']/cml:scalar"
             format="EEE MMM d HH:mm:ss yyyy" />

  <transform process="createWrapperParameter" xpath=".//*[@dictRef='cc:runDate']" />
  <transform process="pullup" xpath=".//cml:list[@cmlx:templateRef='qchem.timestamp']/cml:parameter" />

  <transform process="delete" xpath="cml:list[count(*)=0]" />

  <comment class="example.output" id="qchem.timestamp">
    <module cmlx:templateRef="qchem.timestamp" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx">
      <parameter dictRef="cc:runDate">
        <scalar dataType="xsd:date">2015-05-27T15:12:18Z</scalar>
      </parameter>
    </module>
  </comment>

</template>

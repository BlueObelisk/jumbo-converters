<template id="qchem.hostname"
  repeat="*"
  name="qchem.hostname"
  pattern="\s*Process \d+ of \d+ is on .+"      offset="0"
  endPattern="\s*[^P].+"   endOffset="0" >

  <comment class="example.input" id="qchem.hostname.1">
Process 0 of 1 is on cl056.mun.acenet.ca - thread support 1
  </comment>

  <comment class="example.input" id="qchem.hostname.2">
Process 0 of 2 is on cl092.mun.acenet.ca - thread support 1
Process 1 of 2 is on cl238.mun.acenet.ca - thread support 1
  </comment>

  <record id="qchem.hostname.unparsed" repeat="*">{X,x:UNPARSED}</record>


  <comment class="example.output" id="qchem.hostname.1">
    <module cmlx:templateRef="qchem.hostname" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx">
      <parameter dictRef="cc:numProcs">
          <scalar dataType="xsd:integer">1</scalar>
      </parameter>
      <parameter dictRef="cc:hostname">
        <scalar dataType="xsd:string">cl056.mun.acenet.ca</scalar>
      </parameter>
    </module>
  </comment>

  <comment class="example.output" id="qchem.hostname.2">
    <module cmlx:templateRef="qchem.hostname" xmlns="http://www.xml-cml.org/schema" xmlns:cmlx="http://www.xml-cml.org/schema/cmlx">
      <parameter dictRef="cc:numProcs">
          <scalar dataType="xsd:integer">2</scalar>
      </parameter>
      <parameter dictRef="cc:hostname">
        <list>
          <scalar dataType="xsd:string">cl092.mun.acenet.ca</scalar>
          <scalar dataType="xsd:string">cl238.mun.acenet.ca</scalar>
        </list>
      </parameter>
        
    </module>
  </comment>

</template>

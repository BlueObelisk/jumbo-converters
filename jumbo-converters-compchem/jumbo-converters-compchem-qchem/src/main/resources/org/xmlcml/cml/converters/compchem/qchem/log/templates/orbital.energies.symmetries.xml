<template id="orbital.energies.symmetries"
  repeat="*"
  name="orbital.energies.symmetries"
  pattern="\s+Orbital Energies .a\.u\.. and Symmetries\s*"           offset="-1"
  endPattern="\s*Ground-State Mulliken Net Atomic Charges\s*"        endOffset="0" > 


          <array dataType="xsd:double" size="5" dictRef="cc:alphaOccOrbitalEigenvalues" units="nonSi:hartree" convention="qchemOrbitalSymmetryLabels">-21.012 -1.807 -1.12 -1.12 -0.973</array>
          <array dataType="xsd:double" size="31" dictRef="cc:alphaVirtOrbitalEigenvalues" units="nonSi:hartree" convention="qchemOrbitalSymmetryLabels">-0.149 -0.054 -0.054 0.205 0.205 0.315 0.606 0.722 0.722 0.993 0.993 0.996 1.466 1.54 1.597 1.597 1.85 1.85 2.112 2.426 2.426 2.619 3.263 3.263 3.338 3.786 3.786 5.185 5.672 5.672 51.102</array>
          <array dataType="xsd:double" size="5" dictRef="cc:betaOccOrbitalEigenvalues" units="nonSi:hartree" convention="qchemOrbitalSymmetryLabels">-21.012 -1.807 -1.12 -1.12 -0.973</array>
          <array dataType="xsd:double" size="31" dictRef="cc:betaVirtOrbitalEigenvalues" units="nonSi:hartree" convention="qchemOrbitalSymmetryLabels">-0.149 -0.054 -0.054 0.205 0.205 0.315 0.606 0.722 0.722 0.993 0.993 0.996 1.466 1.54 1.597 1.597 1.85 1.85 2.112 2.426 2.426 2.619 3.263 3.263 3.338 3.786 3.786 5.185 5.672 5.672 51.102</array>
